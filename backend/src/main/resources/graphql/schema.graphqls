type Query {
    searchPerson(name: String!): [Person!]!
}

type Person {
    id: ID!
    name: String!
    platform: String!
    profileUrl: String
    confidence: Float!
    isVerified: Boolean!
    lastSeen: String
}

type SearchResult {
    persons: [Person!]!
    totalCount: Int!
    searchTerm: String!
} 